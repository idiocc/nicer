{
  "name": "nicer",
  "version": "0.0.0-pre",
  "description": "An Http Multipart/Form-Data Request Body Parser.",
  "main": "compiler/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "depack": "depack-dev src/depack -o compiler -a -l -p --externs types/externs.js",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-compiler": "ALAMODE_ENV=test-compiler yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 externs",
    "d1": "typal types/index.js depack/require.js src -c -t types/index.xml",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "build",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/nicer.git"
  },
  "keywords": [
    "nicer"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/nicer/issues"
  },
  "homepage": "https://github.com/idiocc/nicer#readme",
  "devDependencies": {
    "@contexts/http": "^1.5.0",
    "alamode": "^2.3.4",
    "documentary": "^1.27.4",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  }
}
